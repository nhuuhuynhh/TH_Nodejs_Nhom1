<table class="table">
  <thead>
      <tr>
          <th scope="col">STT</th>
          <th scope="col">Tên tài khoản</th>
          <th scope="col">Giới tính</th>
          <th scope="col">Email</th>
          <th scope="col">Địa chỉ</th>
          <th scope="col">Action</th>
      </tr>
  </thead>
  <tbody>
    <% for(let i=0;i<data.rows.length;i++) { %>

      <tr>
        <th scope="row"><%= i+1 %></th>
        <td><%= data.rows[i].username %></td>
        <td><%= data.rows[i].sex %></td>
        <td><%= data.rows[i].gmail %></td>
        <td><%= data.rows[i].address %></td>
        <td>
          <a href="/detail_user/<%= data.rows[i].id %>"><i class="fa fa-info-circle" aria-hidden="true"></i></a> 
          <a href="/edit_user/<%= data.rows[i].id %>"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
          
          <form action="/delete_user" method="POST" class="del">

            <input type="hidden" name="id" value="<%= data.rows[i].id %>">
            <label for="<%= data.rows[i].id %>"><span class="setting"><i class="fa fa-trash" aria-hidden="true"></i></span></label>
            <input type="submit" id="<%= data.rows[i].id %>" value="xoa" style="display: none;"  >
          </form >
        


          
          
        </td>
    </tr>
    
      <% }%>
     
      <!-- Thêm dữ liệu cho các hàng khác -->
  </tbody>
</table>


